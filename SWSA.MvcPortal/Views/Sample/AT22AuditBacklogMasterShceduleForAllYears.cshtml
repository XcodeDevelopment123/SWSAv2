
@{
    ViewData["Title"] = "AT22AuditBacklogMasterShceduleForAllYears";
}
<div class="row">
    <div class="col-md-12">
        <!-- jquery validation -->
        <div class="card card-secondary">
            <div class="card-header">
                <h3 class="card-title">AT2.2 Audit Backlog Master Shcedule For All Years</h3>
            </div>
            <!-- /.card-header -->
            <!-- form start -->
            <div class="card-body">
                <!-- Form 1 -->
                <form>
                    <div class="row">
                        <div class="form-info-label col-12">
                            <label>Audit Backlog Master Info</label>
                        </div>

                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Group</label>
                                <input type="text" class="form-control" />
                            </div>
                        </div>

                        <div class="col-md-5">
                            <div class="form-group">
                                <label>Company Name</label>
                                <input type="text" class="form-control" />
                            </div>
                        </div>

                        <div class="col-md-4">
                            <div class="form-group">
                                <label>Quarter to Do Audit</label>
                                <input type="text" class="form-control" />
                            </div>
                        </div>
                    </div>
                </form>
                <div class="divider-line"></div>

                <!-- Form 2 -->
                <form>
                    <div class="row">
                        <div class="form-info-label col-12">
                            <label>Audit Timing</label>
                        </div>

                        <div class="col-md-2">
                            <div class="form-group">
                                <label>Activity</label>
                                <input type="text" class="form-control" />
                            </div>
                        </div>

                        <div class="col-md-2">
                            <div class="form-group">
                                <label>Year End</label>
                                <select class="form-control select2"></select>
                            </div>
                        </div>

                        <div class="col-md-2">
                            <div class="form-group">
                                <label>Year to do</label>
                                <div class="input-group">
                                    <input type="text" value="yyyy-mm-dd" class="form-control" placeholder="Select date" />
                                    <div class="input-group-append">
                                        <span class="input-group-text"><i class="fa fa-calendar"></i></span>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Move to Active / AEX Sch</label>
                                <div class="input-group">
                                    <input type="text" value="yyyy-mm-dd" class="form-control" placeholder="Select date" />
                                    <div class="input-group-append">
                                        <span class="input-group-text"><i class="fa fa-calendar"></i></span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
                <div class="divider-line"></div>

                <!-- Form 3 -->
                <form>
                    <div class="row">
                        <div class="form-info-label col-12">
                            <label>Document Handling</label>
                        </div>

                        <div class="col-md-4">
                            <div class="form-group">
                                <label>Date Doc In</label>
                                <div class="input-group">
                                    <input disabled type="text" value="yyyy-mm-dd" class="form-control" placeholder="Select date" />
                                    <div class="input-group-append">
                                        <span class="input-group-text"><i class="fa fa-calendar"></i></span>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-md-4">
                            <div class="form-group">
                                <label>Link to A3.1 Doc Control</label>
                                <input type="text" class="form-control" />
                            </div>
                        </div>

                        <div class="col-md-2">
                            <div class="form-group">
                                <label>Acting wk?</label>
                                <select class="form-control select2"></select>
                            </div>
                        </div>

                        <div class="col-md-2">
                            <div class="form-group">
                                <label>Reason why backlog</label>
                                <input type="text" class="form-control" />
                            </div>
                        </div>
                    </div>
                </form>
                <div class="divider-line"></div>
            </div>
        </div>
        <!-- /.card-body -->
        <!-- /.card -->
    </div>
</div>
